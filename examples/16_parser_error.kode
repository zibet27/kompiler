object Point;

object Point (
    x: i32;
    y: i32;
);

fun dotProduct{ p1: Point, p2: Point }: i32 (
    p1.x * p2.x + p1.y * p2.y;
);

fun main{}: i32 (
    Point p1;
    Point p2;
    p1.x = 1;
    p1.y = 2;
    p2.x = 3 // Missing semicolon
    p2.y = 4;
    i32 result = dotProduct{ p1, p2 };
    result;
);
