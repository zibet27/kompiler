alien fun print_string{ s: u8 ptr };

fun main{}: i32 (
    i32 day = 3;
    u8 ptr day_name = switch { day } (
        1 -> "Monday";
        2 -> "Tuesday";
        3 -> "Wednesday";
        4 -> "Thursday";
        5 -> "Friday";
        6 -> "Saturday";
        7 -> "Sunday";
        else -> "Invalid day";
    );

    print_string{ day_name };

    0;
);

// EXPECTED OUTPUT: Wednesday
